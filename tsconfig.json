{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2016", "es2017.object", "dom"],
    "noImplicitAny": true,
    "removeComments": false,
    "sourceMap": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "types": [
      "angular",
      "angular-animate",
      "angular-gettext",
      "angular-route",
      "angular-sanitize",
      "angular-ui-bootstrap",
      "change-case",
      "jointjs",
      "jquery",
      "lodash",
      "modernizr",
      "moment"
    ]
  },
  "exclude": ["types", "node_modules"],
  "awesomeTypescriptLoaderOptions": {
    "forkChecker": true,
    "forkCheckerSilent": true
  }
}
